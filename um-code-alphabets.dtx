    \@@_set_mathalphabet_pos:nnnn  {scr} {g} {up,it} {#1}
    \@@_set_mathalphabet_pos:nnnn  {scr} {o} {up,it} {#1}
  }
%    \end{macrocode}
% These are by default synonyms for the above, but with the STIX
