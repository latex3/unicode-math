    \@@_set_mathalphabet_pos:nnnn  {cal} {I} {up,it} {#1}
    \@@_set_mathalphabet_pos:nnnn  {cal} {L} {up,it} {#1}
    \@@_set_mathalphabet_pos:nnnn  {cal} {M} {up,it} {#1}
    \@@_set_mathalphabet_pos:nnnn  {cal} {R} {up,it} {#1}
  }
%    \end{macrocode}
