    \@@_set_mathalphabet_pos:nnnn  {scr} {F} {up,it} {#1}
    \@@_set_mathalphabet_pos:nnnn  {scr} {H} {up,it} {#1}
    \@@_set_mathalphabet_pos:nnnn  {scr} {I} {up,it} {#1}
    \@@_set_mathalphabet_pos:nnnn  {scr} {L} {up,it} {#1}
